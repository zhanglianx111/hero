{
  "id": "rabbitmq-controller",
  "kind": "ReplicationController",
  "apiVersion": "v1beta1",
  "desiredState": {
    "replicas": 1,
    "replicaSelector": {"name": "rabbitmq"},
    "podTemplate": {
      "desiredState": {
        "manifest": {
          "version": "v1beta1",
          "id": "rabbitmq",
          "containers": [{
            "name": "rabbitmq",
            "image": "dockerfile/rabbitmq",
            "cpu": 100,
            "ports": [{"containerPort": 5672, "hostPort": 5672}]
          }]
        }
      },
      "labels": {
        "name": "rabbitmq",
        "app": "taskQueue"
      }
    }
  },
  "labels": {
    "name": "rabbitmq"
  }
}

